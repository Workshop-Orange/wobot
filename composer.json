{
    "name": "workshop-orange/wobot",
    "description": "The Workshop Orange Bot",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli"],
    "homepage": "https://workshoporange.co/wobot",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/Workshop-Orange/wobot/issues",
        "source": "https://github.com/Workshop-Orange/wobot"
    },
    "authors": [
        {
            "name": "Bryan Gruneberg",
            "email": "hello@bryangruneberg.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
	"guzzlehttp/guzzle": "^7.2",
        "illuminate/database": "^8.40",
        "illuminate/http": "^8.0",
        "illuminate/log": "^8.0",
        "illuminate/redis": "^8.0",
        "intonate/tinker-zero": "^1.2",
        "league/flysystem-aws-s3-v3": "^1.0",
        "newrelic/monolog-enricher": "^2.0",
        "predis/predis": "^1.1",
        "symfony/process": "^5.2",
        "symfony/yaml": "^5.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3",
        "laravel-zero/framework": "^8.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/wobot"]
}
